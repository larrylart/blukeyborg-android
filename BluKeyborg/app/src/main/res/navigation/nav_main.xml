<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/typeFragment">

    <fragment
        android:id="@+id/typeFragment"
        android:name="com.blu.blukeyborg.ui.TypeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_type" />

    <fragment
        android:id="@+id/devicesFragment"
        android:name="com.blu.blukeyborg.ui.DevicesFragment"
        android:label="@string/title_devices"
        tools:layout="@layout/fragment_devices">

        <argument
            android:name="autoLaunchReason"
            app:argType="string"
            android:defaultValue="" />
    </fragment>
	<fragment
		android:id="@+id/setupFragment"
		android:name="com.blu.blukeyborg.ui.SetupFragment"
		android:label="Setup" />	
		
</navigation>
